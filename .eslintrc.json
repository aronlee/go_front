{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "vue",
    "plugins": ["vue"],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "never"
        ],
        "linebreak-style": 0
    }
}